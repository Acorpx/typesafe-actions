{
  "name": "typesafe-actions",
  "version": "2.0.0-rc.1",
  "description": "Typesafe Action Creators for Redux / Flux Architectures (in TypeScript)",
  "author": "Piotr Witek <piotrek.witek@gmail.com> (http://piotrwitek.github.io)",
  "repository": "https://github.com/piotrwitek/typesafe-actions",
  "homepage": "https://github.com/piotrwitek/typesafe-actions",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "engines": {
    "node": ">= 4"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules/ && npm install",
    "clean": "rm -rf dist/",
    "tsc": "tsc -p . --noEmit",
    "tsc:watch": "tsc -p . --noEmit -w",
    "prebuild": "rm -rf dist/ && tsc -p ./tsconfig.declaration.json",
    "build": "tsc -p . && rollup -c",
    "postbuild": "rm -rf out",
    "prelint": "prettier --write 'src/**/*'",
    "lint": "tslint --project './tsconfig.json'",
    "pretest": "tsc -p ./tsconfig.spec.json --noEmit",
    "test": "jest --config jest.config.json ./src",
    "test:watch": "jest --config jest.config.json ./src --watch",
    "prepush": "npm run lint & npm run test",
    "prepublishOnly": "npm run reinstall && npm run lint && npm run tsc && npm run test && npm run build"
  },
  "dependencies": {
    "tslib": "1.9.0"
  },
  "devDependencies": {
    "@types/jest": "22.2.3",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "prettier": "1.12.1",
    "redux": "4.0.0",
    "rollup": "0.58.2",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-filesize": "1.5.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-uglify": "3.0.0",
    "ts-jest": "22.4.5",
    "tslint": "5.10.0",
    "typescript": "2.8.3"
  },
  "keywords": [
    "typescript",
    "typesafe",
    "actions",
    "action-creators",
    "redux",
    "flux",
    "redux-actions",
    "static-typing"
  ]
}
